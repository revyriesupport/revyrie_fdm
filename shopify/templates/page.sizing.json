      "blocks": {
        "template--14650614349896__f83735c5-b724-42b8-a6bd-e8fc5800105d-heading-1": {
          "type": "heading",
          "settings": {
            "heading": "Before we get <em>started<\/em>...",
            "heading_size": "h1",
            "heading_font_size": 38,
            "heading_font_size_mobile": 32
          }
        },
        "template--14650614349896__f83735c5-b724-42b8-a6bd-e8fc5800105d-text-1": {
          "type": "text",
          "settings": {
            "text": "<p>Start by standing in front of a mirror wearing your best-fitting, non-padded underwire bra. You'll also need a soft measuring tape. Remember to measure in inches.<\/p>",
            "content_font_size": 14,
            "content_font_size_mobile": 14
          }
        }
      },
