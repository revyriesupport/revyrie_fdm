.breadcrumb__item.breadcrumb__item{
	color: #A9A9A9;
}

.breadcrumb__item.breadcrumb__item.breadcrumb__item--completed a{
    color: #000000;
}

.livechat_button{
  position: fixed;
  bottom: 10px;
  right: 10px;
  cursor: pointer;
  display: block;
  opacity: 1;
  z-index: 9999;
}

.livechat_button img{
	width: 90px;
}


h1 {
	font-family: 'Dala Floda Roman', "Times New Roman", Times, serif;
	letter-spacing: 0.15rem;
	font-size: 1.4rem;
	color: #000000;
	text-transform: lowercase;
	margin-top: 0px;
	margin-bottom: 15px;
	font-variant-ligatures: none;
}


.main__header {
    padding-bottom: 40px;
}

.order-summary__emphasis {
    color: #000000;
}

.modal__content p {
  font-size: 0.7rem;
}

.breadcrumb__item {
    font-size: 0.7rem;
}

table {
  font-size: 0.75rem;
}

.sidebar {
    color: #000000;
}

.floating-labels .field--show-floating-label .field__input {
    padding-top: 1.6rem;
}

.main__footer {
	margin-top: 60px;
    border-top: 1px solid #f3f3f3;
}

.product__description__name {
  	font-size: 0.7rem;
	font-family: "Neuzeit S LT W01 Book Heavy", Arial, Helvetica, sans-serif;
}

.order-summary__small-text {
    font-size: 0.7rem;
    color: #000000;
}

.modal{
    color: #000000;
}

.review-block__label {
    flex: 0 6em;
    font-size: 10px;
}

.review-block__content {
    padding-right: 1.14286em;
    font-size: 12px;
}

h2 {
	font-family: 'Dala Floda Roman', "Times New Roman", Times, serif;
	letter-spacing: 0.15rem;
	font-size: 1.4rem;
	color: #000000;
	text-transform: lowercase;
	margin-top: 0px;
	margin-bottom: 15px;
	font-variant-ligatures: none;
}

.section__title {
    text-align: center;
}

body{
	background: #ffffff;
	color: #000000;
	font-weight: normal;
	font-style: normal;
	font-size: 0.85rem;
	font-family: "Neuzeit S LT W01 Book", Arial, Helvetica, sans-serif;
	text-rendering: optimizeLegibility;
	-webkit-font-smoothing: antialiased;
  	text-transform: uppercase;
}

.previous-link__icon {
    fill: #d4857b;
  display: none;
}

.edit-link--small {
    font-size: 0.7rem;
}

.display-table .radio__label, .display-table .checkbox__label {
    font-size: 0.7rem;
}

.copyright-text {
    font-size: 0.7rem;
	text-transform: capitalize;
    color: #b7b7b7;
}

.payment-due__price {
    color: #000000;
}

.payment-due-label__total{
    color: #000000;
}

.previous-link__icon:hover, .previous-link__icon:focus {
    fill: #A9A9A9;
}

.step__footer__previous-link, .step__footer__info {
	font-size: 0.7rem;
}

a{
	color:#d4857b;
	outline: none;
}

a:hover, a:focus {
	color:#A9A9A9;
}

a.current {
	color:#000000;
}

a img {outline : none;}

p {
    margin: 0;
    line-height: 2;
    font-size: 0.7rem;
}

b, strong{
	font-weight: 600;
}

ul li{

}

ol li{

}

.shipping-note{
	text-transform: lowercase;
	margin-top: 10px;
	color: #545454;
	font-size: 0.7rem;
}

.shop__logo {
	max-width: 450px;
}

.logo__image{
	margin-bottom: 30px;
    max-height: inherit!important;
}

.logo__image--large {
    max-height: inherit!important;
}

::selection {
	background: initial;
	color: initial;
}

.btn {
	font-family: "Neuzeit S LT W01 Book", Arial, Helvetica, sans-serif;
	background:#000000;
	color: #ffffff;
	border: 1px solid #000000;
	text-align: center;
	font-weight: normal;
	font-size: 0.7rem;
	padding-top: 0.5rem;
	padding-bottom: 0.5rem;
	padding-right: 0.5rem;
	padding-left: 0.5rem;
	border-radius: 0px;
	text-transform: uppercase;
	letter-spacing: 0.025rem;
	height: 2rem;
	margin-top: 0px;
	margin-bottom: 15px;
}

.btn:hover {
	background:#000000;
	color: #ffffff;
}

.field__input {
    border-radius: 0px;
  	font-size: 0.7rem;
}

.display-table .radio__label, .display-table .checkbox__label {
    font-size: 0.7rem;
}

.js .sidebar .btn--disabled {
    cursor: default;
	background-color:#f3f3f3;
	color: #929292;
	border: 1px solid #f3f3f3;
  	box-shadow: none;
}

.breadcrumb__item:after {
    margin: 0px 0.7em;
    margin-bottom: 3px;
}

select{
  text-transform: uppercase;
}

field__input,.no-boxshadow .main .field__input{
  border:1px solid #d9d9d9
}

.boxshadow .main .field__input{
  box-shadow:0 0 0 1px #d9d9d9
}

.main .field__input::-webkit-input-placeholder{
  color:#000000;
  text-transform: uppercase;
}

.main .field__input:-moz-placeholder{
  color:#000000;
  text-transform: uppercase;
}

.main .field__input::-moz-placeholder{
  color:#000000;
  text-transform: uppercase;
}

.main .field__input:-ms-input-placeholder{
  color:#000000;
  text-transform: uppercase;
}

.sidebar .field__input{
  background-color:white;
  color:#000000;
}

.sidebar .field__input::-webkit-input-placeholder{
  color:#000000;
  text-transform: uppercase;
}

.sidebar .field__input:-moz-placeholder{
  color:#000000;
  text-transform: uppercase;
}

.sidebar .field__input::-moz-placeholder{
  color:#000000;
  text-transform: uppercase;
}

.sidebar .field__input:-ms-input-placeholder{
  color:#000000;
  text-transform: uppercase;
}

.alternative-payment-separator::after, .alternative-payment-separator::before {
    background: #f3f3f3;
}



.boxshadow .main .field__input {
    box-shadow: 0 0 0 1px #e6e6e6;
}

.sidebar:after {
    background: #fbfbfb;
    box-shadow: 0 -1px 0 #f3f3f3 inset;
}

  .sidebar:after {
    box-shadow: 1px 0 0 #f3f3f3 inset;
}


