{{ 'section-blog-post.css' | asset_url | stylesheet_tag }}
{% style %}
  article .article-{{ article.id }}.article-image {
    background-image: url('{{ article.image | img_url: '1800x', crop: 'center' }}');
    {% if article.metafields.custom.image_alignment_desktop != blank %}
    background-position: {{ article.metafields.custom.image_alignment_desktop | remove: '"]' | remove: '["' | escape }};
    {% else %}
    background-position: center center;
    {% endif %}
  }
   article.main-article .article-header h1{
font-size: {{ section.settings.heading_font_size }}px;
     padding: 30px 30px 0 30px
  }
   .main-article>*,
  article.main-article .article-body p,
  article.main-article .article-header h1,
  article.main-article .article-header .copy,
  article.main-article .article-body h2,
  .outline.btn{
    color: {{ section.settings.text_color}};
  }
  
  article.main-article .article-body a,
  article.main-article .article-details li a{
    color: #d4857b;
    font-weight: 500;
    text-decoration: none;
    outline: none;
  }
  article.main-article a:hover,
  article.main-article a:focus {
    color: #000;
    cursor: pointer;
}
  article.main-article p{
      margin-bottom: 15px;
  }
  
  .main-article .social-sharing svg{
  fill:{{ section.settings.text_color}};
  }
  article.main-article .article-body h2{
   font-size:32px;
  margin-bottom: 10px;
    font-weight:500;
        text-transform: lowercase;
  }
  .article-body .row.bottom-margin{
    margin-bottom: 30px;
  }
  .my-60{
    margin-top:60px;
    margin-bottom:60px;
  }
{% endstyle %}
<article role="article" itemscope itemtype="http://schema.org/Article" class="main-article">
  {% if article.image %}
    <div class="article-{{ article.id }} main-article-img-wrap article-image"></div>
  {% endif %}
  <div class="article-header{% if article.image %} has-image{% endif %}">
    <div class="row">
      <div class="medium-12 columns">
        {% for tag in article.tags %}
          {% if tag contains 'category:' %}
            <div class="article-category">{{ tag | remove: 'category:' }}</div>
          {% endif %}
        {% endfor %}
        <h1>{{ article.title }}</h1>
        {% if article.content contains '<!-- subtitle -->' %}
          <div class="copy article-subtitle">
            {{ article.content | split: '<!-- subtitle -->' | last | split: '<!-- end subtitle -->' | first }}
          </div>
        {% endif %}
        <ul class="article-details">
          <li>
            <a href="{{ blog.url }}">{% include 'icon-arrow-left' %} Back</a>
          </li>
          {%- if section.settings.blog_show_date -%}
            <li class="circle-divider">{{ article.published_at | date: '%B %d, %Y' }}</li>
          {%- endif -%}
          {%- if section.settings.blog_show_author -%}
            <li>{{ article.author }}</li>
          {%- endif -%}
        </ul>
        <!-- article-date -->
      </div>
      <!-- medium-12 columns -->
    </div>
    <!-- row -->
  </div>
  <!-- article-header -->
  <div class="articleBody" itemprop="articleBody">
    {% if article.content contains '<!-- section-1 -->' %}
